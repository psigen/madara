
project (NDDS_Transport_IDL) : using_ndds_idl, build_files {

  requires += ndds

  custom_only = 1
  
  Build_Files {
    $(MADARA_ROOT)/default.features
    NDDS_Transport.mpc
    using_ndds.mpb
  }
  
  ndds_notao_ts_flags += -d include/madara/transport/ndds
  
  NDDSNoTaoTypeSupport_Files {
    include/madara/transport/ndds/Ndds_Knowledge_Update.idl
  }
}
