<group>
    <setup>
        <parallel />
        <realtime />
        <timing />
        <domain>barrier_benchmarks</domain>
    </setup>
    <sleep>
        <debug />
        <realtime />
        <timing />
        <host>$(HOST)</host>
        <domain>barrier_benchmarks</domain>
        <barrier name="ten_process" />
        <id>1</id>
        <processes>10</processes>
        <delay>5</delay>
        <stderr>$(RESULTS)/host2_tentotal_sleep1.err</stderr>
        <stdout>$(RESULTS)/host2_tentotal_sleep1.out</stdout>
    </sleep>
    <sleep>
        <debug />
        <realtime />
        <timing />
        <host>$(HOST)</host>
        <domain>barrier_benchmarks</domain>
